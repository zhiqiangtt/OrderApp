<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.orderapp">

    <application
        android:name="org.litepal.LitePalApplication"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".Login.login"
            android:label="@string/app_name"
            android:theme="@style/AppTheme.NoActionBar" ></activity>
        <activity
            android:name=".Login.regist"
            android:label="@string/app_name"
            android:theme="@style/AppTheme.NoActionBar" ></activity>
        <activity android:name=".index"></activity>
        <activity android:name=".EditUser"></activity>
        <activity android:name=".AddFood" ></activity>
        <activity android:name=".EditFood" ></activity>
        <activity
            android:name=".IndexOp.daincai"
            android:label="@string/app_name"
            android:theme="@style/AppTheme" />
        <activity android:name=".IndexOp.liushui"></activity>
    </application>

</manifest>